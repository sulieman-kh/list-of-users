<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />

    <title>Список юзеров</title>
    <style>
      * { margin: 0; padding: 0; } window.scrollTo({ top: 0, left: 0, behavior:
      'smooth' }) html { box-sizing: border-box; zoom: 90%; } body { height:
      100vh; position: relative; } .ms-n5 { margin-left: -42px; margin-top: 4px;
      } .ml-5 { margin-left: 3px; } .fs-24{font-size: 24px;} .fs-14 { font-size:
      14px; } body.modal-open { padding-right: 0px; } .modal-backdrop.fade.show
      { width: 100%; height: 100%; } .modal-backdrop.show { opacity: .6; }
      .modal-backdrop { background-color: #BCC3D0; } .color-grey { color:
      #8189A3 } .color-plumbum{ color: #262C40; } form.card.shadow{ transition:
      all .2s ease-in-out; } form.card.shadow:hover { transform:
      translate3d(0px, -5px, 0px); }
    </style>
  </head>
  <body>
    <div class="container pt-5">
      <div class="row">
        <div class="col-md-12 mx-auto">
          <form
            class="input-group"
            method="POST"
            enctype="application/json"
            action="/"
          >
            <input
              class="form-control border-end-0 border rounded-pill"
              name="search"
              type="search"
              value=""
              novalidate
            />
            <span class="input-group-append">
              <button
                class="btn btn-outline-secondary bg-white border-bottom-0 border-white rounded-pill ms-n5"
                type="submit"
              >
                <i class="fa fa-search" style="color:#432eab"></i>
              </button>
            </span>
          </form>
        </div>
      </div>

      <div class="col-md-12 pt-5 pb-5">
        {{{body}}}
      </div>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
      integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/js/bootstrap.min.js"
      integrity="sha384-oesi62hOLfzrys4LxRF63OJCXdXDipiYWBnvTl9Y9/TRlw5xlKIEHpNyvvDShgf/"
      crossorigin="anonymous"
    ></script>
  </body>
</html>